{
  "name": "my/package",
  "version": "1.0.0",

  "types": {
    "BarVisualization": {
      "base": "pentaho/visual/base/Visualization",
      // "isAbstract": false, // default
      "alias": "myBar",

      // Applicable to browsing info, and also to the component itself.
      //"styleClass": "my-package-bar-visualization", // default

      // Forward declaration of browsing information.
      "browse": {
        "enabled": true, // default is false; configurable
        "i18n": true,    // ["./i18n/BarVisualization.browse"]; configurable
        "styles": true   // ["./theme/BarVisualization.browse.css"]; configurable

        // Provide defaults.
        // Better provided by configuration.
        //"category": "bar",
        //"ordinal": 10,

        // Can provide i18n defaults. Not advisable.
        // Better provided in associated i18n file.
        // Can also be _configured_, using the "i18n" property.
        //"label": "My Bar",
        //"description": "My Bar chart",
        //"helpUrl": "//www.docs.io/my/package/BarVisualization"
      }
    }
  },
  "instances": {
    "config": {
      "type": "pentaho/config/spec/IRuleSet"
    }
  }
}