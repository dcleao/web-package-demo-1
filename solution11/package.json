{
  "name": "@my/bar",
  "version": "1.0.0",
  "map": {
    "@pentaho": "@hitachivantara/pentaho-core",
    "@pentaho-visual": "@hitachivantara/pentaho-visual"
  },
  "config": {
    "@pentaho/modules": {
      
      "@my/bar/Model": {
        "base": "@pentaho-visual/base/Model",
        "info": true,
        "i18n": true,
        "config": {
          "::annotations": {
            "@pentaho-visual/DefaultView": "./View"
            // @angular
            // @react
          }
        }
      },

      "@my/bar/View": {
        // No real need to specify base.
        // "base": "@pentaho-visual/IView"

        "i18n": true,
        "config": {
          "::annotations": {
            "@pentaho/theme/Themed": {"themes": {"ruby": true}}
          }
        }
      },

      "@my/bar/Model.info": {
        // No real need to specify type.
        // "type": "@pentaho/spec/IUIInfo",

        "i18n": true,
        
        // When value is unspecified, it would be = merge(baseValue, i18n, config)
        "value": {
          "category": "bar",
          // "isAdvanced": false,
        },
        "config": {
          // "isBrowsable": true, 
          "::annotations": {
            "@pentaho/theme/Themed": {"themes": {"ruby": true}}
          }
        }
      }
    }
  }
}
